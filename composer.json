{
    "name": "ekyna/dpd",
    "description": "PHP component for DPD shipment",
    "type": "component",
    "authors": [
        {
            "name": "Etienne Dauvergne",
            "homepage": "http://ekyna.com"
        }
    ],
    "keywords": [
        "php",
        "dpd",
        "shipment",
        "web service"
    ],
    "homepage": "https://github.com/ekyna/Dpd",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-soap": "*",
        "ext-gd": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ekyna\\Component\\Dpd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekyna\\Component\\Dpd\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
